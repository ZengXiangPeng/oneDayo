  border-radius: 10rpx;
  margin:auto;
  margin-top: 40rpx;
  line-height: 60rpx;
}
.ent{
  width: 61%;
  height: 60rpx;
  background-color: rgb(250,250,210);
  box-shadow: #778899 0px 0px 8px;
  margin:auto;
  margin-top: 40rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 32rpx;
  line-height: 60rpx;
}